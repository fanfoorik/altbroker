{
  "presets": [
    "es2015",
    "react",
    "stage-2"
  ],
  "plugins": [
    [
      "import", { "libraryName": "antd", "style": "less" }
    ],
    [
      "react-css-modules", {
        "context" : "./src",
        "generateScopedName": "[name]__[local]___[hash:base64:5]",
        "filetypes": {
          ".module.less": {
            "syntax": "postcss-less"
          }
        }
      }
    ]
  ]
}